<DataObject name='GetRecordExchange'>
	<Intro>
		The GetRecordExchange message requests that a StudentRecordExchange object set be published for a given student and sending agency.
	</Intro>
	<Item>
		<Element>GetRecordExchange</Element>
		<Description>

		</Description>
	</Item>
	<Item>
		<Element>StudentId</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The StateProvinceId of the student</p>
		</Description>
		<Type ref='DataModel' name='StateProvinceId'/>
	</Item>
	<Item>
		<Element>SendingAgencyId</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The StateProvinceId of the sending agency</p>
		</Description>
		<Type ref='DataModel' name='StateProvinceId'/>
	</Item>
	<Item>
		<Element>ReceivingAgencyId</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The StateProvinceId of the receiving agency</p>
		</Description>
		<Type ref='DataModel' name='StateProvinceId'/>
	</Item>
	<Item>
		<Element>ExtendedParameters</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>Optional list of additional implementation-dependent parameters</p>
		</Description>
		<Type ref='DataModel' name='ExtendedParameters'/>
	</Item>
	<xhtml:Example name='GetRecordExchange' xmlns=''>
		<GetRecordExchange>
			<StudentId>10009600</StudentId>
			<SendingAgencyId>884</SendingAgencyId>
			<ReceivingAgencyId>601</ReceivingAgencyId>
			<ExtendedParameters>
				<ExtendedParameter>
					<SIF_Name>Options</SIF_Name>
					<SIF_Value>0x0001</SIF_Value>
				</ExtendedParameter>
				<ExtendedParameter>
					<SIF_Name>ExchangeType</SIF_Name>
					<SIF_Value>Transcript</SIF_Value>
				</ExtendedParameter>
			</ExtendedParameters>
		</GetRecordExchange>
	</xhtml:Example>
</DataObject>
