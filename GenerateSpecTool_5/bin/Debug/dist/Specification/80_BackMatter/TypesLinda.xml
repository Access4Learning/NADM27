<CommonElement name="BellPeriodListType" type="true" dm="true">
	<Item>
		<Element>BellPeriodListType</Element>
		<Characteristics>M</Characteristics>
		<Description>
			<p>The bell periods for this school year bell schedule.</p>
		</Description>
		<List mode="List"/>
	</Item>
	<Item>
		<Element>BellPeriod</Element>
		<Characteristics>MR</Characteristics>
		<Description>
			<p>An entry for each unique bell period for the bell schedule.</p>
		</Description>
		<Type ref="CommonTypes" name="BellPeriodType"/>
