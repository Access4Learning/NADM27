<CommonElement name="BellPeriodType" type="true" dm="true">
	<Item>
		<Element>BellPeriodType</Element>
		<Characteristics>O</Characteristics>
		<Description>
			<p>An entry for each unique bell period for the bell schedule.</p>
		</Description>
	</Item>
 <Item>
    <Element>TimetableDayIdentifier</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The unique identifier for the timetable day used for the bell period (from the day list above).</p>
    </Description>
    <Type name='xs:token'/>
  </Item>

  <Item>
    <Element>TimetablePeriodIdentifier</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The unique identifier for the timetable period used for the bell period (from the period list above).</p>
    </Description>
    <Type name='xs:normalizedString'/>
  </Item>

  <Item>
    <Element>StartTime</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The starting time for the bell period.</p>
    </Description>
    <Type name='xs:time'/>
  </Item>

  <Item>
    <Element>EndTime</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The ending time for the bell period.</p>
    </Description>
    <Type name='xs:time'/>
  </Item>

  <Item>
    <Element>RegularSchoolPeriod</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Indicates if the bell period is part of the regular school day (i.e. is not a before or after school or break period).</p>
    </Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>

  <Item>
    <Element>InstructionalMinutes</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The number of minutes to be counted for instruction for the bell period.</p>
    </Description>
    <Type name='xs:unsignedInt'/>
  </Item>

  <Item>
    <Element>UseInAttendanceCalculations</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Indicates if the bell period should be included in attendance calculations.</p>
    </Description>
    <Values>
      <Value>
        <Code>Yes</Code>
      </Value>
      <Value>
        <Code>No</Code>
      </Value>
    </Values>
  </Item>
</CommonElement>
