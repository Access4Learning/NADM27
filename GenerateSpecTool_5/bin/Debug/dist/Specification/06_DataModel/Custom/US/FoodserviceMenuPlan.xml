<DataObject name="FoodserviceMenuPlan">
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>FoodserviceMenuPlan</Element>
    <Description>
      <p>This object communicates the foodservice menu plan for a school site for a given meal program, grade level range and date.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies this menu plan</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the school for this menu plan</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>GradeLevels</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="GradeLevels"></Type>
  </Item>
  <Item>
    <Element>MenuPlanDate</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Date of menu plan</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>Program</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="Program"></Type>
  </Item>
  <Item>
    <Element>Description</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>Description of menu plan</p>
    </Description>
    <Type name="xs:string"></Type>
  </Item>
  <Item>
    <Element>ProjectedParticipation</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Feeding figure projected</p>
    </Description>
    <Type name="xs:string"></Type>
  </Item>
  <Item>
    <Element>MenuPlanQuantities</Element>
    <Characteristics>M</Characteristics>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/MenuPlanQuantity</Element>
    <Characteristics>OR</Characteristics>
    <Description>
      <p>Quantity of item in this menu plan</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  <Item>
    <Attribute>FoodserviceItemPortionRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Item portion amount</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Attribute>MenuCategory</Attribute>
    <Characteristics>M</Characteristics>
    <Values>
      <Value>
        <Code>ActualMeal</Code>
      </Value>
      <Value>
        <Code>ProjectedMeal</Code>
      </Value>
      <Value>
        <Code>ActualAlaCarte</Code>
      </Value>
      <Value>
        <Code>ProjectedAlaCarte</Code>
      </Value>
      <Value>
        <Code>Produced</Code>
      </Value>
      <Value>
        <Code>LeftOver</Code>
      </Value>
      <Value>
        <Code>Wasted</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>

  <xhtml:Example xmlns="">
    <FoodserviceMenuPlan RefId="EF521958AB474559B2AE7BD82647C66B">
      <SchoolInfoRefId>FF521958AB474559B2AE7BD82647C66B</SchoolInfoRefId>
      <GradeLevels>
        <GradeLevel>
          <Code>01</Code>
        </GradeLevel>
        <GradeLevel>
          <Code>02</Code>
        </GradeLevel>
        <GradeLevel>
          <Code>03</Code>
        </GradeLevel>
        <GradeLevel>
          <Code>04</Code>
        </GradeLevel>
        <GradeLevel>
          <Code>05</Code>
        </GradeLevel>
      </GradeLevels>
      <MenuPlanDate>2001-10-31</MenuPlanDate>
      <Program Type="Foodservice">NSLP</Program>
      <Description>Halloween Menu</Description>
      <ProjectedParticipation>1000</ProjectedParticipation>
      <MenuPlanQuantities>
        <MenuPlanQuantity FoodserviceItemPortionRefId="D9837A776DD626777872361DD118873C" MenuCategory="ActualMeal">13</MenuPlanQuantity>
        <MenuPlanQuantity FoodserviceItemPortionRefId="A08CC23B059C400CAA9F9BBD3DFF94F1" MenuCategory="ActualAlaCarte">16</MenuPlanQuantity>
      </MenuPlanQuantities>
    </FoodserviceMenuPlan>
  </xhtml:Example>
</DataObject>