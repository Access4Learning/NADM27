<DataObject name="FoodserviceMealPrices">
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>FoodserviceMealPrices</Element>
    <Description>
      <p>This object is a site's meal prices for each grade program, meal status, meal type and grade level.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies this meal price.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the school this meal price belongs to</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>Program</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="Program"></Type>
  </Item>
  <Item>
    <Element>Prices</Element>
    <Characteristics>M</Characteristics>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/Price</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Price of the meal</p>
    </Description>
  </Item>
  <Item>
    <Element>//Value</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The price value</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"/>
  </Item>
  <Item>
    <Element>//MealStatus</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="MealStatus"></Type>
  </Item>
  <Item>
    <Element>//MealType</Element>
    <Characteristics>M</Characteristics>
    <Type ref="CommonTypes" name="MealType"></Type>
  </Item>
  <Item>
    <Element>//GradeLevels</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="GradeLevels"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <FoodserviceMealPrices RefId="EF521958AB474559B2AE7BD82647C66B">
      <SchoolInfoRefId>FF521958AB474559B2AE7BD82647C66B</SchoolInfoRefId>
      <Program Type="Foodservice">NSLP</Program>
      <Prices>
        <Price>
          <Value Currency="USD">999.99</Value>
          <MealStatus Type="Free" />
          <MealType>FirstMeal</MealType>
          <GradeLevels>
            <GradeLevel>
              <Code>09</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>10</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>11</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>12</Code>
            </GradeLevel>
          </GradeLevels>
        </Price>
        <Price>
          <Value>999.99</Value>
          <MealStatus Type="Free" />
          <MealType>SecondMeal</MealType>
          <GradeLevels>
            <GradeLevel>
              <Code>09</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>10</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>11</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>12</Code>
            </GradeLevel>
          </GradeLevels>
        </Price>
        <Price>
          <Value>999.99</Value>
          <MealStatus Type="Reduced" />
          <MealType>FirstMeal</MealType>
          <GradeLevels>
            <GradeLevel>
              <Code>09</Code>
