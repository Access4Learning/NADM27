<DataObject name="EmployeePersonal">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>EmployeePersonal</Element>
    <Description>
      <p>This object contains basic HR information for employees.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The GUID that uniquely identifies this employee.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>StateProvinceId</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The state-assigned identifier for this staff member.</p>
    </Description>
    <Type ref="DataModel" name="StateProvinceId"></Type>
  </Item>
  <Item>
    <Element>OtherIdList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>List all "other" Ids associated with the employee.</p>
    </Description>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/OtherId</Element>
    <Characteristics>OR</Characteristics>
    <Description>
      <p>This field can hold a SSN, state or locally assigned Id.</p>
    </Description>
    <Type name="xs:normalizedString"></Type>
  </Item>
  <Item>
    <Attribute>Type</Attribute>
    <Characteristics>M</Characteristics>
    <Values>
      <Value>
        <Code>SSN</Code>
      </Value>
      <Value>
        <Code>State</Code>
      </Value>
      <Value>
        <Code>Local</Code>
      </Value>
      <Value>
        <Code>Other</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>Name</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Name of employee.</p>
    </Description>
    <Type ref="DataModel" name="Name"></Type>
  </Item>
  <Item>
    <Element>Demographics</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>This element contains information related to employee demographics.</p>
    </Description>
    <Type ref="DataModel" name="Demographics"></Type>
  </Item>
  <Item>
    <Element>AddressList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The employee's address(es).</p>
    </Description>
    <Type ref="DataModel" name="AddressList"></Type>
  </Item>
  <Item>
    <Element>PhoneNumberList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The employee's phone number(s).</p>
    </Description>
    <Type ref="DataModel" name="PhoneNumberList"></Type>
  </Item>
  <Item>
    <Element>EmailList</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The employee's e-mail address(es).</p>
    </Description>
    <Type ref="DataModel" name="EmailList"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <EmployeePersonal RefId="CD590651225DAC3859DEA3458BC39583">
      <OtherIdList>
        <OtherId Type="SSN">590651225</OtherId>
        <OtherId Type="Other">3333</OtherId>
      </OtherIdList>
      <Name Type="04">
        <LastName>Stone</LastName>
        <FirstName>Alicia</FirstName>
      </Name>
      <Demographics>
        <Gender>F</Gender>
      </Demographics>
      <AddressList>
        <Address Type="0123">
          <Street>
            <Line1>600 Royal Oak</Line1>
          </Street>
          <City>Chicago</City>
          <StateProvince>IL</StateProvince>
          <Country>US</Country>
          <PostalCode>60601</PostalCode>
        </Address>
      </AddressList>
      <PhoneNumberList>
        <PhoneNumber Type="0096">
          <Number>(312) 555-1234</Number>
        </PhoneNumber>
      </PhoneNumberList>
      <EmailList>
        <Email Type="Primary">stone@royaloak.org</Email>
      </EmailList>
    </EmployeePersonal>
  </xhtml:Example>
</DataObject>