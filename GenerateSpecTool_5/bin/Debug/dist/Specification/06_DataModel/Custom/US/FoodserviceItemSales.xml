<DataObject name="FoodserviceItemSales">
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>FoodserviceItemSales</Element>
    <Description>
      <p>This object gives the sales details for the items sold for a specified date.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies this object</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the school for this object</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>Date</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Date of sales</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>Program</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="Program"></Type>
  </Item>
  <Item>
    <Element>ItemQuantities</Element>
    <Characteristics>M</Characteristics>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/ItemQuantity</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Quantity of item amounts sold</p>
    </Description>
  </Item>
  <Item>
    <Attribute>FoodserviceItemPortionRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID of FoodserviceItemPortion object</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>//Value</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The value of item quantity</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  <Item>
    <Element>//MealType</Element>
    <Characteristics>M</Characteristics>
    <Type ref="CommonTypes" name="MealType"></Type>
  </Item>
  <Item>
    <Element>//GradeLevels</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="GradeLevels"></Type>
  </Item>
  <Item>
    <Element>//Cost</Element>
    <Characteristics>O</Characteristics>
    <Description>
      <p>The cost (monetary value) of the item sold.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"/>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <FoodserviceItemSales RefId="F7E8FF23F3A1471BA9D0B0E22A0839C6">
      <SchoolInfoRefId>B72A99FFC3624AD2ADD197CC5A4A2563</SchoolInfoRefId>
      <Date>2001-09-13</Date>
      <Program Type="Foodservice">NSLP</Program>
      <ItemQuantities>
        <ItemQuantity FoodserviceItemPortionRefId="D38E8D805B664B7CB389CFE2C11B145F">
          <Value>35</Value>
          <MealType>FirstMeal</MealType>
          <GradeLevels>
            <GradeLevel>
              <Code>01</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>02</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>03</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>04</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>05</Code>
            </GradeLevel>
          </GradeLevels>
        </ItemQuantity>
        <ItemQuantity FoodserviceItemPortionRefId="A08CC23B059C400CAA9F9BBD3DFF94F1">
          <Value>152</Value>
          <MealType>AlaCarte</MealType>
          <GradeLevels>
            <GradeLevel>
              <Code>01</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>02</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>03</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>04</Code>
            </GradeLevel>
            <GradeLevel>
              <Code>05</Code>
            </GradeLevel>
          </GradeLevels>
        </ItemQuantity>
      </ItemQuantities>
    </FoodserviceItemSales>
  </xhtml:Example>
</DataObject>