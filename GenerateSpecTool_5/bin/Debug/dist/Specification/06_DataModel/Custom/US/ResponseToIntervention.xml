</Targets>
	<EntryDate>2011-09-01</EntryDate>
	<ProjectedExitDate>2012-05-12</ProjectedExitDate>
	<FrequencyTime>
		<Code>S001</Code>
		<Duration UnitOfMeasure="Minutes">120</Duration>
	</FrequencyTime>
</ResponseToIntervention>
   

  </xhtml:Example>
</DataObject>