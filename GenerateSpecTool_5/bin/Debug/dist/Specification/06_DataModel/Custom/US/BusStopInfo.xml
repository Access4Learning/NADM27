<DataObject name="BusStopInfo">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>BusStopInfo</Element>
    <Description>
      <p>This object contains information about a bus stop.</p>
    </Description>
  </Item>
  <Item>

    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies this location.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>Description</Element>

    <Characteristics>M</Characteristics>
    <Description>
      <p>An explanation of this location.</p>
    </Description>
    <Type name="xs:string"></Type>
  </Item>
  <Item>
    <Element>GridLocation</Element>

    <Characteristics>O</Characteristics>
    <Description>
      <p>The location of the bus stop.</p>
    </Description>
    <Type ref="DataModel" name="GridLocation"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>

    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <BusStopInfo RefId="D3E34B359D75101A8C3D00AA001A1652">
      <Description>Elm St @ Pine Ave</Description>
      <GridLocation>
        <Latitude>41.850000</Latitude>
        <Longitude>-87.650000</Longitude>
      </GridLocation>
    </BusStopInfo>
  </xhtml:Example>
</DataObject>