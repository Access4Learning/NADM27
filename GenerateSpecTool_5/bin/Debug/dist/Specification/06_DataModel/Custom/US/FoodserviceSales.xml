<DataObject name="FoodserviceSales">
  <EventsReported>true</EventsReported>
  <Key>@RefId</Key>
  <Item>
    <Element>FoodserviceSales</Element>
    <Description>
      <p>This object communicates the daily sales information.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>GUID that identifies the daily meal sales object</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Element>SchoolInfoRefId</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>School for which the sales object applies</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>Date</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Date on which the sales occurred.</p>
    </Description>
    <Type name="xs:date"></Type>
  </Item>
  <Item>
    <Element>Program</Element>
    <Characteristics>M</Characteristics>
    <Type ref="DataModel" name="Program"></Type>
  </Item>
  <Item>
    <Element>SalesAmounts</Element>
    <Characteristics>M</Characteristics>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/SalesAmount</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Monetary value</p>
      <p>(count * cost per unit) done for all grade levels</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"/>
  </Item>
  <Item>
    <Attribute>Type</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Type of SalesAmount. This denotes the form of incoming funds for the sales done.</p>
    </Description>
    <Values>
      <Value>
        <Code>Cash</Code>
      </Value>
      <Value>
        <Code>Account</Code>
      </Value>
      <Value>
        <Code>Charged</Code>
      </Value>
      <Value>
        <Code>Earned</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Attribute>AccountType</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>
        Type of account to which sales amounts apply.  This is set to <code>NA</code> when <code>Type</code> is set as <code>Earned</code>.
      </p>
    </Description>
    <Values>
      <Value>
        <Code>PrepaidCash</Code>
      </Value>
      <Value>
        <Code>BreakfastCredit</Code>
      </Value>
      <Value>
        <Code>LunchCredit</Code>
      </Value>
      <Value>
        <Code>NA</Code>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <FoodserviceSales RefId="E57A732918E1473F80C2CFA7774DAA6A">
      <SchoolInfoRefId>D21CF29B7FB44965A9CF950E1E60B21E</SchoolInfoRefId>
      <Date>2001-10-31</Date>
      <Program Type="Foodservice">NSLP</Program>
      <SalesAmounts>
        <SalesAmount Type="Cash" AccountType="NA" Currency="USD">9999.99</SalesAmount>
        <SalesAmount Type="Account" AccountType="PrepaidCash">9999.99</SalesAmount>
        <SalesAmount Type="Charged" AccountType="PrepaidCash">9999.99</SalesAmount>
        <SalesAmount Type="Charged" AccountType="BreakfastCredit">9999.99</SalesAmount>
        <SalesAmount Type="Charged" AccountType="LunchCredit">9999.99</SalesAmount>
        <SalesAmount Type="Earned" AccountType="NA">9999.99</SalesAmount>
      </SalesAmounts>
    </FoodserviceSales>
  </xhtml:Example>
</DataObject>