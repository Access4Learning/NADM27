  <DataObject name="EmployeeCredential">
      <EventsReported>false</EventsReported>
      <Item>
        <Element>EmployeeCredential</Element>
        <Description>
          <p>
            This object contains HR, certification and teaching assignment information for elementary and secondary
            professional personnel for state reporting. It is a snapshot object used for vertical reporting of data related to K-12
            elementary and secondary professional employees.
          </p>
        </Description>
      </Item>
      
	  <Item>
		<Attribute>RefId</Attribute>
		<Characteristics>M</Characteristics>
		<Description>
		  <p>A GUID that identifies an instance of this object. This RefId is a “transient” value; that is, it SHOULD be generated by the provider at the time the object is provided in a Response message.  This value MUST NOT be used in a later Request for the object.</p>
		</Description>
		<Type ref="CommonTypes" name="RefIdType"></Type>
	  </Item>      
      
      <Item>
        <Element>ReportDate</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Date that report snapshot was generated</p>
        </Description>
        <Type name="xs:date"></Type>
      </Item>
      <Item>
        <Element>SchoolYear</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>School year for which the information is applicable, expressed as the four-digit year in which the school year ends (e.g., "2004" for the 2003-04 school year).</p>
        </Description>
        <Type ref="CommonTypes" name="SchoolYearType"></Type>
      </Item>
      <Item>
        <Element>StateProvinceId</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>State assigned reporting unit number</p>
        </Description>
        <Type ref="DataModel" name="StateProvinceId"></Type>
      </Item>
      <Item>
        <Element>SSN</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Employee social security number</p>
        </Description>
        <Type name="xs:normalizedString"></Type>
      </Item>
      <Item>
        <Element>Name</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Name of employee.</p>
        </Description>
        <Type ref="DataModel" name="Name"></Type>
      </Item>
      <Item>
        <Element>Demographics</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>This element contains information related to employee demographics.</p>
        </Description>
        <Type ref="DataModel" name="Demographics"></Type>
      </Item>
      <Item>
        <Element>Race</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Primary employee's race</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0849RaceType"></Type>
      </Item>
      <Item>
        <Element>Hispanic</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>Is this employee Hispanic or Latino?</p>
        </Description>
        <Values>
          <Value>
            <Code>Yes</Code>
          </Value>
          <Value>
            <Code>No</Code>
          </Value>
        </Values>
      </Item>
      <Item>
        <Element>Certifications</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Employee certifications</p>
        </Description>
        <List mode="List"/>
      </Item>
      <Item>
        <Element>/Certification</Element>
        <Characteristics>MR</Characteristics>
        <Description>
          <p>Employee certification area information</p>
        </Description>
      </Item>
      <Item>
        <Element>//Description</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Certification description as defined by state</p>
        </Description>
        <Type name="xs:string"></Type>
      </Item>
      <Item>
        <Element>//AreaAuthorized</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>Area in which employee has certification</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0421TeachingFieldOrAreaAuthorizedType"></Type>
      </Item>
      <Item>
        <Element>Salary</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Employee's salary</p>
        </Description>
        <Type ref="CommonTypes" name="MonetaryAmountType"/>
      </Item>
      <Item>
        <Element>Status</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Employee professional position status</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0613EmploymentStatusType"></Type>
      </Item>
      <Item>
        <Element>Leave</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Is this employee on leave this year?</p>
        </Description>
        <Values>
          <Value>
            <Code>Yes</Code>
          </Value>
          <Value>
            <Code>No</Code>
          </Value>
        </Values>
      </Item>
      <Item>
        <Element>TotalYears</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Total number of years employee has been in a professional position</p>
        </Description>
        <Type name="xs:decimal"></Type>
      </Item>
      <Item>
        <Element>UnitYears</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Total number of years at current LEA</p>
        </Description>
        <Type name="xs:decimal"></Type>
      </Item>
      <Item>
        <Element>Education</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Highest level of education attained by employee.</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0395TeachingCredentialBasisType"></Type>
      </Item>
      <Item>
        <Element>TeachingCredentialType</Element>
        <Characteristics>OR</Characteristics>
        <Description>
          <p>Category in which teacher is authorized to teach</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0394TeachingCredentialTypeType"></Type>
      </Item>
      <Item>
        <Element>TerminationCode</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>If appropriate, employment separation reason</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0434EmploymentSeparationReasonType"></Type>
      </Item>
      <Item>
        <Element>BackgroundChecks</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>List of background checks performed on employee</p>
        </Description>
        <List mode="List"/>
      </Item>
      <Item>
        <Element>/BackgroundCheck</Element>
        <Characteristics>OR</Characteristics>
        <Description>
          <p>Information about background check done on employee</p>
        </Description>
      </Item>
      <Item>
        <Element>//Type</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Background check type</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0401BackgroundCheckTypeType"></Type>
      </Item>
      <Item>
        <Element>//Date</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Date on which background type was checked</p>
        </Description>
        <Type name="xs:date"></Type>
      </Item>
      <Item>
        <Element>//Description</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>Description of background check performed</p>
        </Description>
        <Type name="xs:string"></Type>
      </Item>
      <Item>
        <Element>Assignments</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>List of employee assignments to schools</p>
        </Description>
        <List mode="List"/>
      </Item>
      <Item>
        <Element>/Assignment</Element>
        <Characteristics>MR</Characteristics>
        <Description>
          <p>Information about the school and assignment of employee</p>
        </Description>
      </Item>
      <Item>
        <Element>//SchoolInfoRefId</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>References SchoolInfo object to determine school in which this assignment pertains.</p>
        </Description>
        <Type ref="CommonTypes" name="IdRefType"></Type>
      </Item>
      <Item>
        <Element>//GradeLevels</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Grade which is assigned to this employee (e.g. 12)</p>
        </Description>
        <Type ref="DataModel" name="GradeLevels"></Type>
      </Item>
      <Item>
        <Element>//JobFunction</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>The purpose of the activity in school as related to students</p>
        </Description>
      </Item>
      <Item>
        <Element>///Code</Element>
        <Characteristics>M</Characteristics>
        <Description>
          <p>Code representing the type of job function.</p>
        </Description>
        <Type ref="ExternalCodeSets" name="NCES0568FunctionTypeType"></Type>
      </Item>
      <Item>
        <Element>///OtherCodeList</Element>
        <Characteristics>O</Characteristics>
        <Type ref="DataModel" name="OtherCodeList"></Type>
      </Item>
      <Item>
        <Element>//FTE</Element>
        <Characteristics>O</Characteristics>
        <Description>
          <p>Full-time equivalent for this school</p>
        </Description>
        <Type name="xs:decimal"></Type>
      </Item>
      <Item>
        <Element>SIF_Metadata</Element>
        <Characteristics>O</Characteristics>
        <Type ref="DataModel" name="SIF_Metadata"></Type>
      </Item>
      <Item>
        <Element>SIF_ExtendedElements</Element>
        <Characteristics>O</Characteristics>
        <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
      </Item>
      <xhtml:Example xmlns="">
        <EmployeeCredential RefId="359D75101AD0A9D7A8C3DAD0A85103A2">
          <ReportDate>2004-01-15</ReportDate>
          <SchoolYear>2004</SchoolYear>
          <StateProvinceId>123456</StateProvinceId>
          <SSN>590651225</SSN>
          <Name Type="04">
            <LastName>Silverberg</LastName>
            <FirstName>Betty</FirstName>
          </Name>
          <Demographics>
            <Gender>F</Gender>
          </Demographics>
          <Race>0998</Race>
          <Hispanic>No</Hispanic>
          <Certifications>
            <Certification>
              <Description>Secondary Math</Description>
              <AreaAuthorized>1166</AreaAuthorized>
            </Certification>
          </Certifications>
          <Salary Currency="USD">35000</Salary>
          <Status>1379</Status>
          <Leave>No</Leave>
          <TotalYears>12</TotalYears>
          <UnitYears>12</UnitYears>
          <Education>1237</Education>
          <TeachingCredentialType>1222</TeachingCredentialType>
          <TerminationCode>1387</TerminationCode>
          <BackgroundChecks>
            <BackgroundCheck>
              <Type>1420</Type>
              <Date>2004-01-10</Date>
              <Description>FBI background check</Description>
            </BackgroundCheck>
          </BackgroundChecks>
          <Assignments>
            <Assignment>
              <SchoolInfoRefId>72348623487623488AB9827349872349</SchoolInfoRefId>
              <GradeLevels>
                <GradeLevel>
                  <Code>11</Code>
                </GradeLevel>
                <GradeLevel>
                  <Code>12</Code>
                </GradeLevel>
              </GradeLevels>
              <JobFunction>
                <Code>1000</Code>
              </JobFunction>
              <FTE>1.0</FTE>
            </Assignment>
          </Assignments>
        </EmployeeCredential>
      </xhtml:Example>
    </DataObject>