<DataObject name="EmployeeWage">
  <Key>@RefId</Key>
  <EventsReported>true</EventsReported>
  <Item>
    <Element>EmployeeWage</Element>
    <Description>
      <p>This object contains HR/payroll employee information used to define how to pay an employee per pay cycle.</p>
    </Description>
  </Item>
  <Item>
    <Attribute>RefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The GUID of the employee's pay information.</p>
    </Description>
    <Type ref="CommonTypes" name="RefIdType"></Type>
  </Item>
  <Item>
    <Attribute>EmployeePersonalRefId</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>The GUID that refers to the EmployeePersonal object.</p>
    </Description>
    <Type ref="CommonTypes" name="IdRefType"></Type>
  </Item>
  <Item>
    <Element>PayRates</Element>
    <Characteristics>M</Characteristics>
    <List mode="List">
    </List>
  </Item>
  <Item>
    <Element>/PayRate</Element>
    <Characteristics>MR</Characteristics>
    <Description>
      <p>Per pay distributions</p>
    </Description>
  </Item>
  <Item>
    <Attribute>Type</Attribute>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Type of pay</p>
    </Description>
    <Values>
      <Value>
        <Code>P</Code>
        <Text>Per pay</Text>
      </Value>
      <Value>
        <Code>H</Code>
        <Text>Hourly</Text>
      </Value>
      <Value>
        <Code>D</Code>
        <Text>Per diem</Text>
      </Value>
      <Value>
        <Code>X</Code>
        <Text>Extra pay</Text>
      </Value>
    </Values>
  </Item>
  <Item>
    <Element>//Amount</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Pay amount.</p>
    </Description>
    <Type ref="CommonTypes" name="MonetaryAmountType"/>
  </Item>
  <Item>
    <Element>//Percentage</Element>
    <Characteristics>M</Characteristics>
    <Description>
      <p>Percentage of pay this represents.</p>
    </Description>
    <Type name="xs:decimal"></Type>
  </Item>
  <Item>
    <Element>SIF_Metadata</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_Metadata"></Type>
  </Item>
  <Item>
    <Element>SIF_ExtendedElements</Element>
    <Characteristics>O</Characteristics>
    <Type ref="DataModel" name="SIF_ExtendedElements"></Type>
  </Item>
  <xhtml:Example xmlns="">
    <EmployeeWage RefId="CDF90651225DAC3859D2A3458BC39583" EmployeePersonalRefId="CD490651225DAC3859DEA3458BC39522">
      <PayRates>
        <PayRate Type="P">
          <Amount Currency="USD">355.45</Amount>
          <Percentage>100</Percentage>
        </PayRate>
      </PayRates>
    </EmployeeWage>
  </xhtml:Example>
</DataObject>